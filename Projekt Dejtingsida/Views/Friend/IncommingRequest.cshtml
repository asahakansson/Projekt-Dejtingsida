@model IEnumerable<Projekt_Dejtingsida.Models.FriendRequestModels>

@{
    ViewBag.Title = "IncommingRequests";
}

<h2>Incomming requests</h2>
<table class="table">
    <tr>
        <th>
            From:
        </th>
        <th>
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Person1)
            </td>
            <td>
                @Html.ActionLink("Accept", "", "", null, new { @class = "btn btn-primary" })/@Html.ActionLink("Decline", "", "", null, new { @class = "btn btn-primary" })
            </td>
        </tr>
    }

</table>
